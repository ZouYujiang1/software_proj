{"ast":null,"code":"import axios from \"axios\";\nimport Router from \"@/router\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  name: \"Ch_status\",\n\n  data() {\n    return {\n      message: \"\",\n      current: \"\",\n      cars: [],\n      current_index: 0,\n      timer: ''\n    };\n  },\n\n  mounted() {\n    let vm = this;\n    const route = useRoute(); //从路由中获取充电桩id\n\n    vm.current_id = route.params.id;\n    vm.get_data();\n    vm.timer = setInterval(() => vm.get_data(), 1000);\n  },\n\n  methods: {\n    search(event) {\n      let search_id = document.getElementById('in').value;\n\n      for (const i in this.message) {\n        if (this.message[i].id == search_id) {\n          this.current = this.message[i];\n          this.current_index = parseInt(i);\n          this.current_id = search_id;\n          break;\n        }\n      }\n    },\n\n    get_data() {\n      let vm = this;\n      axios.get('http://127.0.0.1:5000/admin/charger/service').then(function (response) {\n        console.log(response);\n        response = response.data['data'];\n        vm.message = response;\n        vm.current = response[0];\n\n        for (const i in response) {\n          if (response[i].id == vm.current_id) {\n            vm.current = response[i];\n            vm.current_index = parseInt(i);\n            break;\n          }\n        }\n\n        if (vm.current === null) {\n          vm.current = response[0];\n          alert(\"wrong id\");\n          return;\n        }\n\n        vm.cars = vm.current.in_service;\n      }).catch(function (error) {\n        // 请求失败处理\n        console.log(error);\n      });\n    },\n\n    data_pre(event) {\n      this.cars = this.current.in_service;\n    },\n\n    next_one(event) {\n      console.log(this.message.length, this.current_index);\n\n      if (this.message.length <= this.current_index + 1) {\n        return;\n      }\n\n      this.current_index = this.current_index + 1;\n      this.current = this.message[this.current_index];\n      this.current_id = this.current.id;\n      this.data_pre();\n    },\n\n    last_one(event) {\n      if (0 > this.current_index - 1) {\n        return;\n      }\n\n      this.current_index = this.current_index - 1;\n      this.current = this.message[this.current_index];\n      this.current_id = this.current.id;\n      this.data_pre();\n    }\n\n  },\n\n  updated() {},\n\n  destroyed() {\n    clearInterval(this.timer);\n  }\n\n};","map":{"version":3,"mappings":"AAwCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,SAAQC,QAAR,QAAuB,YAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,OAAO,EAAE,EAFJ;MAGLC,IAAI,EAAE,EAHD;MAILC,aAAa,EAAE,CAJV;MAKLC,KAAK,EAAE;IALF,CAAP;EAOD,CAVY;;EAWbC,OAAO,GAAG;IACR,IAAIC,EAAC,GAAI,IAAT;IACA,MAAMC,KAAI,GAAIV,QAAQ,EAAtB,CAFQ,CAGR;;IACAS,EAAE,CAACE,UAAH,GAAgBD,KAAK,CAACE,MAAN,CAAaC,EAA7B;IACAJ,EAAE,CAACK,QAAH;IACAL,EAAE,CAACF,KAAH,GAAWQ,WAAW,CAAC,MAAMN,EAAE,CAACK,QAAH,EAAP,EAClB,IADkB,CAAtB;EAGD,CApBY;;EAqBbE,OAAO,EAAC;IACNC,MAAM,CAACC,KAAD,EAAO;MACX,IAAIC,SAAQ,GAAIC,QAAQ,CAACC,cAAT,CAAwB,IAAxB,EAA8BC,KAA9C;;MACA,KAAI,MAAMC,CAAV,IAAe,KAAKpB,OAApB,EAA4B;QAC1B,IAAG,KAAKA,OAAL,CAAaoB,CAAb,EAAgBV,EAAhB,IAAsBM,SAAzB,EAAmC;UACjC,KAAKf,OAAL,GAAe,KAAKD,OAAL,CAAaoB,CAAb,CAAf;UACA,KAAKjB,aAAL,GAAqBkB,QAAQ,CAACD,CAAD,CAA7B;UACA,KAAKZ,UAAL,GAAkBQ,SAAlB;UACA;QACF;MACF;IACD,CAXK;;IAYNL,QAAQ,GAAE;MACR,IAAIL,EAAC,GAAI,IAAT;MACEX,KAAI,CACC2B,GADL,CACS,6CADT,EAEKC,IAFL,CAEU,UAAUC,QAAV,EAAmB;QACvBC,OAAO,CAACC,GAAR,CAAYF,QAAZ;QACAA,QAAO,GAAIA,QAAQ,CAACzB,IAAT,CAAc,MAAd,CAAX;QACAO,EAAE,CAACN,OAAH,GAAawB,QAAb;QACAlB,EAAE,CAACL,OAAH,GAAauB,QAAQ,CAAC,CAAD,CAArB;;QACA,KAAI,MAAMJ,CAAV,IAAeI,QAAf,EAAwB;UACtB,IAAGA,QAAQ,CAACJ,CAAD,CAAR,CAAYV,EAAZ,IAAkBJ,EAAE,CAACE,UAAxB,EAAmC;YACjCF,EAAE,CAACL,OAAH,GAAauB,QAAQ,CAACJ,CAAD,CAArB;YACAd,EAAE,CAACH,aAAH,GAAmBkB,QAAQ,CAACD,CAAD,CAA3B;YACA;UACF;QACF;;QAEA,IAAGd,EAAE,CAACL,OAAH,KAAe,IAAlB,EAAuB;UACrBK,EAAE,CAACL,OAAH,GAAauB,QAAQ,CAAC,CAAD,CAArB;UACAG,KAAK,CAAC,UA